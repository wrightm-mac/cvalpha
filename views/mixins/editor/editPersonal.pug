mixin editPersonal
  table#cvSectionPersonal
    tr
      th.editorColumnVisible
      th.editorSectionTitle(colspan=2)= info.personal.title
    each item in info.personal.values
      tr(data-id=item.id)
        td.editorColumnVisible
          input(id=name, type='checkbox', checked=item.visible)
        td.editorColumnPersonalName
          span.editorClickable(data-id='name_' + item.id)= item.name
        td.editorColumnPersonalValue
          span.editorClickable(data-id='value_' + item.id)= item.value
