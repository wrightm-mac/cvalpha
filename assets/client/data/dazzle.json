{
  "sections": [{
    "title": "Page",
    "parts": [{
        "name": "Font",
        "type": "font",
        "selectors": "#content",
        "style": "font-family"
      }, {
        "name": "Font Size",
        "type": "number",
        "min": 10,
        "max": 50,
        "selectors": "#content",
        "style": "font-size"
      }, {
        "name": "Page Margin",
        "type": "number",
        "min": 2,
        "max": 180,
        "mulitplier": 3,
        "selectors": "#content",
        "style": "padding",
        "translate": "0 $px 0 $px",
        "initial": "padding-left"
      }, {
        "name": "Background",
        "type": "color",
        "selectors": [
          "body",
          "#content"
        ],
        "style": "background-color"
      }, {
        "name": "Line Height",
        "type": "float",
        "min": 11,
        "max": 70,
        "mulitplier": 100,
        "selectors": "#content #editor",
        "style": "line-height",
        "translate": "$px"
      }, {
        "name": "Letter Spacing",
        "type": "float",
        "min": 0.1,
        "max": 15,
        "mulitplier": 100,
        "selectors": "#content #editor",
        "style": "letter-spacing",
        "translate": "$px"
      }, {
        "name": "Section Spacing",
        "type": "number",
        "min": 5,
        "max": 130,
        "selectors": "div.cvSpacer",
        "style": "height",
        "translate": "$px"
      }]
    }, {
      "title": "Titles",
      "container": "#editor",
      "parts": [{
        "name": "Color",
        "type": "color",
        "selectors": "th.editorSectionTitle",
        "style": "color"
      }, {
        "name": "Background",
        "type": "color",
        "selectors": "th.editorSectionTitle",
        "style": "background-color"
      }, {
        "name": "Border Width",
        "type": "number",
        "min": 0,
        "max": 12,
        "selectors": "th.editorSectionTitle",
        "style": "border-width",
        "translate": "$px"
      }, {
        "name": "Border Color",
        "type": "color",
        "selectors": [
          "th.editorSectionTitle",
          "th.editorSectionTitle div.editorSectionTitleUnderline"
        ],
        "style": "border-color"
      }, {
        "name": "Border Radius",
        "type": "number",
        "min": 0,
        "max": 45,
        "selectors": "th.editorSectionTitle",
        "style": "border-radius",
        "translate": "$px $px 0 $px"
      }, {
        "name": "Underline",
        "type": "number",
        "min": 0,
        "max": 8,
        "selectors": "th.editorSectionTitle div.editorSectionTitleUnderline",
        "style": "border-width",
        "translate": "0 0 $px 0",
        "initial": "border-bottom-width"
      }, {
        "name": "Opacity",
        "type": "float",
        "min": 0,
        "max": 1,
        "multiplier": 100,
        "selectors": "th.editorSectionTitle",
        "style": "opacity"
      }]
    }, {
      "title": "Section",
      "container": "#editor",
      "parts": [{
        "name": "Text Color",
        "type": "color",
        "selectors": ".cvSection table",
        "style": "color"
      }, {
        "name": "Background",
        "type": "color",
        "selectors": ".cvSection table",
        "style": "background-color"
      }, {
        "name": "Padding",
        "type": "number",
        "min": 0,
        "max": 20,
        "selectors": ".cvSection table",
        "style": "padding",
        "translate": "$px"
      }, {
        "name": "Border Width",
        "type": "number",
        "min": 0,
        "max": 12,
        "selectors": ".cvSection table",
        "style": "border-width",
        "translate": "$px"
      }, {
        "name": "Border Color",
        "type": "color",
        "selectors": ".cvSection table",
        "style": "border-color"
      }, {
        "name": "Border Radius",
        "type": "number",
        "min": 0,
        "max": 45,
        "selectors": ".cvSection table",
        "style": "border-radius",
        "translate": "$px $px 0 $px"
      }, {
        "name": "Opacity",
        "type": "float",
        "min": 0,
        "max": 1,
        "multiplier": 100,
        "selectors": ".cvSection table",
        "style": "opacity"
      }]
    }, {
      "title": "Items",
      "container": "#editor",
      "parts": [{
        "name": "Background",
        "type": "color",
        "selectors": ".editorTableRow .editorColumn",
        "style": "background-color"
      }, {
        "name": "Border Width",
        "type": "number",
        "min": 0,
        "max": 12,
        "selectors": ".editorTableRow .editorColumn",
        "style": "border-width",
        "translate": "$px"
      }, {
        "name": "Border Color",
        "type": "color",
        "selectors": ".editorTableRow .editorColumn",
        "style": "border-color"
      }, {
        "name": "Border Radius",
        "type": "number",
        "min": 0,
        "max": 45,
        "selectors": ".editorTableRow .editorColumn",
        "style": "border-radius",
        "translate": "$px $px 0 $px"
      }, {
        "name": "Opacity",
        "type": "float",
        "min": 0,
        "max": 1,
        "multiplier": 100,
        "selectors": ".editorTableRow .editorColumn",
        "style": "opacity"
      }]
    }, {
      "title": "Privacy",
      "parts": [{
        "name": "Public",
        "type": "toggle",
        "value": true
      }]
    }, {
      "title": "Social Media",
      "parts": [{
        "name": "Twitter",
        "type": "toggle",
        "value": true,
        "badge": "/images/twitter.png"
      }, {
        "name": "Facebook",
        "type": "toggle",
        "value": true,
        "badge": "/images/facebook.png"
      }, {
        "name": "Instagram",
        "type": "toggle",
        "value": false,
        "badge": "/images/instagram.png"
      }]
    }]
}